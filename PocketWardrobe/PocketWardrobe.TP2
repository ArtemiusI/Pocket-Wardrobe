COPY ~%MOD_FOLDER%/itm/C0WARDRO.ITM~ ~override~
SAY NAME2 ~The Amazing Pocket Wardrobe~
SAY DESC ~This magical mirror allows you to freely change your character's appearance.~

ACTION_IF MOD_IS_INSTALLED ~ArtisansKitpack.tp2~ ~10003~ BEGIN

  OUTER_SPRINT monk_class ~OR(3) Class(Player1,MONK) Kit(Player1,C0_SACRED_FIST) Kit(Player1,C0_ENLIGHTENED_FIST)~

COPY_EXISTING ~C0WARDRO.ITM~ ~override~
		LPF ADD_ITEM_EQEFFECT
			INT_VAR
				opcode = 319
				target = 1
				timing = 2
				parameter1 = IDS_OF_SYMBOL (~KIT~ ~C0_ENLIGHTENED_FIST~)
				parameter2 = 9
		END
		LPF ADD_ITEM_EQEFFECT
			INT_VAR
				opcode = 319
				target = 1
				timing = 2
				parameter1 = IDS_OF_SYMBOL (~KIT~ ~C0_SACRED_FIST~)
				parameter2 = 9
		END

END ELSE BEGIN

  OUTER_SPRINT monk_class ~Class(Player1,MONK)~

END

COMPILE EVALUATE_BUFFER ~%MOD_FOLDER%/d/dialog.d~

OUTER_SET pocket_wardrobe = RESOLVE_STR_REF (~Makeover~)
APPEND ~ITEMDIAL.2da~ ~C0WARDRO    %pocket_wardrobe%    C0WARDRO~

ACTION_FOR_EACH baldurbcs IN baldur bdbaldur baldur25 BEGIN

  ACTION_IF FILE_EXISTS_IN_GAME ~%baldurbcs%.bcs~ BEGIN
    EXTEND_BOTTOM ~%baldurbcs%.bcs~ ~%MOD_FOLDER%/baf/baldur.baf~
  END

END
